{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": ["name", "endpoint", "maxContextWindow", "maxOutputTokens", "requestTimeout", "apiUrl"],
  "properties": {
    "name": {
      "type": "string",
      "minLength": 1
    },
    "endpoint": {
      "type": "string",
      "minLength": 1
    },
    "maxContextWindow": {
      "type": "number",
      "exclusiveMinimum": 0,
      "maximum": 999999999
    },
    "maxOutputTokens": {
      "type": "number",
      "exclusiveMinimum": 0,
      "maximum": 999999999
    },
    "requestTimeout": {
      "type": "number",
      "exclusiveMinimum": 0,
      "maximum": 999999999
    },
    "apiKey": {
      "type": "string"
    },
    "apiUrl": {
      "type": "string",
      "minLength": 1
    }
  }
}